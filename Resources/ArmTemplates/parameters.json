{
    "$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentParameters.json#",
    "contentVersion": "1.0.0.0",
    "parameters": {
        "appSharedResourceGroup": {
            "value": "__appSharedResourceGroup__"
        },
        "appSharedStorageAccountName": {
            "value": "__appSharedStorageAccountName__"
        },
        "appServicePlanSuffix": {
            "value": "__AppServicePlanSuffix__"
        },
        "functionAppName": {
            "value": "__FunctionAppName__"
        },
        "CosmosDbResourceGroup": {
            "value": "__appSharedResourceGroup__"
        },
        "cosmosDBName": {
            "value": "__appSharedCosmosDBName__"
        },
        "cosmosDbKey": {
            "value": "__appSharedCosmosPrimaryKey__"
        },
        "environment": {
            "value": "__ReleaseEnvironmentName__",
            "metadata": {
                "comment": "Bug in tokenisation task prevents referencing Release.EnvironmentName directly.  https://github.com/TotalALM/VSTS-Tasks/issues/29"
            }
        },
        "cosmosDbCollectionName": {
            "value": "__cosmosDbCollectionName__"
        },
        "eventstoreDatabaseNeedsCreation": {
            "value": __CosmosDbDatabaseDoesntExist__
        },
        "eventstoreDatabaseThroughput": {
            "value": __eventstoreDatabaseThroughput__
        },
        "ApimResourceGroup": {
            "value": "__SharedResourceGroupName__"
        },
        "ApimLoggerName": {
            "value": "__ApimLoggerName__"
        },
        "ApimProductInstanceName": {
            "value": "__ApimProductInstanceName__"
        },
        "ApimServiceName": {
            "value": "__ApimServiceName__"
        },
        "ApiName": {
            "value": "__ApiName__"
        },
        "workspaceName": {
            "value": "__sharedLogAnalyticsWorkspace__"
        },
        "workspaceResourceGroup": {
            "value": "__sharedLogAnalyticsResourceGroup__"
        }
    }
}